  <Step enable="True" id="89" name="Comment">
    <Text>Original: Go to Layout [ “Contact List” ; Animation: None ]</Text>
  </Step>
  <Step enable="True" id="6" name="Go to Layout">
    <LayoutDestination value="LayoutNameByCalc"/>
    <Layout>
      <Calculation><![CDATA[“Contact List”]]></Calculation>
    </Layout>
  </Step>
  <Step enable="True" id="89" name="Comment">
    <Text>Original: Perform Script [ Specified: From list ; “Trigger | Sort Contact List [Get Script Parameter]” ; Parameter: $$PLATFORM ]</Text>
  </Step>
  <Step enable="True" id="1" name="Perform Script">
    <Calculation><![CDATA[$$PLATFORM]]></Calculation>
    <DisplayCalculation>
      <Calculation><![CDATA[$$PLATFORM]]></Calculation>
    </DisplayCalculation>
    <Script name="“Trigger | Sort Contact List [Get Script Parameter]”" id="1"/>
  </Step>
  <Step enable="True" id="68" name="If">
    <Calculation><![CDATA[Get ( SystemPlatform ) = 3]]></Calculation>
  </Step>
  <Step enable="True" id="43" name="Print">
    <NoInteract state="False"/>
    <Restore state="True"/>
  </Step>
  <Step enable="True" id="121" name="Close Window">
    <LimitToWindowsOfCurrentFile state="True"/>
    <Window value="Current"/>
  </Step>
  <Step enable="True" id="69" name="Else"/>
