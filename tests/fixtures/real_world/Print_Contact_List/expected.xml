      <Calculation><![CDATA[1]]></Calculation>
    </Repetition>
  </Step>
  <Step enable="True" id="122" name="New Window">
    <LayoutDestination value="CurrentLayout"/>
    <Name>
      <Calculation><![CDATA["Contact List"]]></Calculation>
    </Name>
    <Height>
      <Calculation><![CDATA[820]]></Calculation>
    </Height>
    <Width>
      <Calculation><![CDATA[630]]></Calculation>
    </Width>
    <DistanceFromTop>
      <Calculation><![CDATA[Get ( WindowTop ) + 20]]></Calculation>
    </DistanceFromTop>
    <DistanceFromLeft>
      <Calculation><![CDATA[Get ( WindowLeft ) + Get ( WindowWidth ) - 650]]></Calculation>
    </DistanceFromLeft>
    <NewWndStyles Style="Floating" Close="Yes" Minimize="No" Maximize="No" Resize="Yes" Styles="2147549952"/>
